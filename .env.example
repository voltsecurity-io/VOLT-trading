# VOLT Trading System - API Keys Template
# Copy this file to .env and fill in your API keys

# ===========================================
# LLM Providers (using any-llm)
# ===========================================

# Ollama (local - no API key needed)
OLLAMA_HOST=http://localhost:11434

# OpenAI
OPENAI_API_KEY=

# Anthropic (Claude)
ANTHROPIC_API_KEY=

# Mistral AI
MISTRAL_API_KEY=

# Google AI (Gemini)
GOOGLE_API_KEY=

# ===========================================
# Exchange APIs
# ===========================================

# Binance (use testnet keys for testing)
BINANCE_API_KEY=
BINANCE_API_SECRET=

# For sandbox/testnet
BINANCE_TESTNET=true

# ===========================================
# Data APIs (optional)
# ===========================================

# CryptoPanic for sentiment (https://cryptopanic.com/)
CRYPTOPANIC_API_KEY=

# CoinGecko for market data (free tier available)
COINGECKO_API_KEY=

# FRED for macro data (https://fred.stlouisfed.org/)
FRED_API_KEY=

# ===========================================
# System Settings
# ===========================================

# Trading mode: production, dryrun, paper
VOLT_MODE=dryrun

# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
