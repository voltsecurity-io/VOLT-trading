{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "VOLT Trading: Run",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "VOLT Trading: Control Start",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/control.py",
            "args": ["start"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "VOLT Trading: Control Status",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/control.py",
            "args": ["status"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
